{
  "name": "contential",
  "description": "Contential - JavaScript Client",
  "version": "0.0.42",
  "dependencies": {
    "@contential/api": "latest",
    "@contential/prompt": "latest"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./*"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "require": "./index.js"
    },
    "./chat": {
      "types": "./chat.d.ts",
      "import": "./chat.js",
      "require": "./chat.js"
    },
    "./prompt": {
      "types": "./prompt.d.ts",
      "import": "./prompt.js",
      "require": "./prompt.js"
    }
  }
}
